- name: epy_block_4
  id: epy_block
  parameters:
    K_dB: '15'
    _source_code: "import numpy as np\nfrom gnuradio import gr\n\nclass realistic_channel_model(gr.sync_block):\n\
      \    \"\"\"\n    Canal realista con:\n      - Path loss\n      - AWGN\n    \
      \  - Multipath\n      - Desplazamiento de frecuencia\n      - Fading lento (shadowing\
      \ lognormal)\n      - Doppler variable tipo Jakes\n      - Fading Rician\n \
      \     - Desfase inicial aleatorio (nuevo)\n      - Ruido impulsivo ocasional\
      \ (nuevo)\n    \"\"\"\n\n    def __init__(self,\n                 pathloss_dB=118.2,\n\
      \                 noise_voltage=0.14,\n                 freq_offset=0.005,\n\
      \                 epsilon=1.0,\n                 taps=[1.0, 0.25-0.25j, 0.50\
      \ + 0.10j, -0.3 + 0.2j],\n                 seed=0,\n                 shadow_std_dB=3.0,\n\
      \                 shadow_rate=0.001,\n                 doppler_hz=20.0,\n  \
      \               samp_rate=250e3,\n                 K_dB=15,\n              \
      \   impulsive_prob=0,      # Probabilidad de r\xE1faga\n                 impulsive_power=1.5):\
      \      # Potencia relativa del impulso\n        gr.sync_block.__init__(self,\n\
      \            name=\"Realistic Channel Model\",\n            in_sig=[np.complex64],\n\
      \            out_sig=[np.complex64])\n\n        # === Par\xE1metros base ===\n\
      \        self.pathloss_dB = float(pathloss_dB)\n        self.noise_voltage =\
      \ float(noise_voltage)\n        self.freq_offset = float(freq_offset)\n    \
      \    self.epsilon = float(epsilon)\n        self.taps = np.array(taps, dtype=np.complex64)\n\
      \        self.seed = int(seed)\n        self.shadow_std_dB = float(shadow_std_dB)\n\
      \        self.shadow_rate = float(shadow_rate)\n        self.doppler_hz = float(doppler_hz)\n\
      \        self.samp_rate = float(samp_rate)\n        self.K_dB = float(K_dB)\n\
      \        self.impulsive_prob = float(impulsive_prob)\n        self.impulsive_power\
      \ = float(impulsive_power)\n\n        np.random.seed(self.seed)\n\n        #\
      \ === Estado interno ===\n        self.phase = 2 * np.pi * np.random.rand()\
      \  # Desfase inicial aleatorio\n        self.delay_buffer = np.zeros(len(self.taps)-1,\
      \ dtype=np.complex64)\n        self.pathloss_linear = 10 ** (-self.pathloss_dB\
      \ / 20.0)\n        self.shadow_gain = 1.0\n\n        # Par\xE1metros Doppler\
      \ (Jakes)\n        self.doppler_phase = 0.0\n        self.doppler_speed = self.doppler_hz\n\
      \        self.doppler_update_rate = 0.1  # velocidad de cambio relativa\n\n\
      \    def work(self, input_items, output_items):\n        x = input_items[0]\n\
      \        y = np.zeros_like(x, dtype=np.complex64)\n\n        # === Multipath\
      \ ===\n        x_full = np.concatenate([self.delay_buffer, x])\n        for\
      \ i in range(len(self.taps)):\n            y += self.taps[i] * x_full[i:i+len(x)]\n\
      \        self.delay_buffer = x_full[-(len(self.taps)-1):]\n\n        # === Desplazamiento\
      \ de frecuencia + Doppler Jakes ===\n        n = np.arange(len(y))\n       \
      \ phase_inc_offset = 2 * np.pi * self.freq_offset  # offset fijo\n\n       \
      \ doppler_random = np.sin(self.doppler_phase)\n        phase_inc_doppler = 2\
      \ * np.pi * self.doppler_speed * doppler_random / self.samp_rate\n        total_phase\
      \ = self.phase + n * (phase_inc_offset + phase_inc_doppler)\n\n        y *=\
      \ np.exp(1j * total_phase)\n        self.phase = (total_phase[-1] + phase_inc_offset\
      \ + phase_inc_doppler) % (2*np.pi)\n\n        self.doppler_phase += self.doppler_update_rate\
      \ * np.random.uniform(0.8, 1.2)\n        if self.doppler_phase > 2*np.pi:\n\
      \            self.doppler_phase -= 2*np.pi\n\n        # === Fading Rician ===\n\
      \        K = 10 ** (self.K_dB / 10.0)\n        N = len(y)\n        h = np.sqrt(K/(K+1))\
      \ + np.sqrt(1/(K+1)) * (\n            np.random.normal(size=N) + 1j * np.random.normal(size=N)\n\
      \        ) / np.sqrt(2)\n        y *= h.astype(np.complex64)\n\n        # ===\
      \ Retardo (epsilon) ===\n        if self.epsilon != 1.0:\n            t_in =\
      \ np.arange(len(y))\n            t_out = np.arange(0, len(y), self.epsilon)\n\
      \            y = np.interp(t_out, t_in, y.real) + 1j * np.interp(t_out, t_in,\
      \ y.imag)\n            y = y.astype(np.complex64)\n\n        # === Path loss\
      \ ===\n        y *= self.pathloss_linear\n\n        # === Fading lento (shadowing\
      \ lognormal) ===\n        shadow_dB = self.shadow_std_dB * np.random.randn()\n\
      \        alpha = np.exp(-self.shadow_rate)\n        self.shadow_gain = alpha\
      \ * self.shadow_gain + (1 - alpha) * (10 ** (shadow_dB / 20.0))\n        y *=\
      \ self.shadow_gain\n\n        # === Ruido AWGN ===\n        if self.noise_voltage\
      \ > 0.0:\n            noise = (np.random.normal(0, self.noise_voltage, len(y))\
      \ +\n                     1j * np.random.normal(0, self.noise_voltage, len(y)))\
      \ / np.sqrt(2)\n            y += noise.astype(np.complex64)\n\n        # ===\
      \ Ruido impulsivo ocasional ===\n        if np.random.rand() < self.impulsive_prob:\n\
      \            burst = (np.random.randn(len(y)) + 1j * np.random.randn(len(y)))\
      \ * self.impulsive_power\n            y += burst.astype(np.complex64)\n\n  \
      \      # === Salida ===\n        output_items[0][:len(y)] = y\n        return\
      \ len(y)\n"
    affinity: ''
    alias: ''
    comment: ''
    doppler_hz: '20'
    epsilon: '1'
    freq_offset: '0.005'
    impulsive_power: '1.5'
    impulsive_prob: '0'
    maxoutbuf: '0'
    minoutbuf: '0'
