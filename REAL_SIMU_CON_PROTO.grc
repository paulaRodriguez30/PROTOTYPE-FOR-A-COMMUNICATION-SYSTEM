- name: epy_block_3
  id: epy_block
  parameters:
    _source_code: "import numpy as np\nimport pmt\nfrom gnuradio import gr\n\nclass\
      \ image_receiver(gr.basic_block):\n    def __init__(self):\n        gr.basic_block.__init__(\n\
      \            self,\n            name='image_receiver',\n            in_sig=None,\n\
      \            out_sig=None\n        )\n        self.message_port_register_in(pmt.intern(\"\
      msg_in\"))\n        self.set_msg_handler(pmt.intern(\"msg_in\"), self.handle_msg)\n\
      \n        self.receiving = False\n        self.received = bytearray()\n\n  \
      \  def handle_msg(self, msg):\n        vec = pmt.cdr(msg)\n        data = bytearray(pmt.u8vector_elements(vec))\n\
      \n        if data == b\"START\":\n            self.receiving = True\n      \
      \      self.received = bytearray()\n            print(\"[RX] Recibiendo imagen...\"\
      )\n\n        elif data == b\"END\":\n            if self.receiving:\n      \
      \          with open(\"\\\\Users\\\\USUARIO\\\\Music\\\\aaaff.jpg\", \"wb\"\
      ) as f:\n                    f.write(self.received)\n                print(f\"\
      [RX] Imagen reconstruida correctamente. Total: {len(self.received)} bytes\"\
      )\n                self.receiving = False\n            else:\n             \
      \   print(\"[RX] END recibido sin START\")\n\n        elif self.receiving:\n\
      \            self.received += data\n            print(f\"[RX] Chunk recibido:\
      \ {len(data)} bytes\")\n"
    affinity: ''
    alias: ''
    comment: ''
    maxoutbuf: '0'
