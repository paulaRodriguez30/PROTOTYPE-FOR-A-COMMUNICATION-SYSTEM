    pathloss_dB: '118.2'
    samp_rate: samp_rate
    seed: '0'
    shadow_rate: '0.001'
    shadow_std_dB: '3.0'
    taps: taps
  states:
    _io_cache: ('Realistic Channel Model', 'realistic_channel_model', [('pathloss_dB',
